<% CreateImageURL=function(homeserverUrl,id) { return `${homeserverUrl}${id.slice(0,2)}/${id.slice(2,4)}/${id.slice(4)}`; } %>
<%
PackRatingClass=function(rating) {
    switch (rating.toLowerCase()) {
        case "safe":
            return "text-green-400"
            break
        case "questionable":
            return "text-yellow-500"
            break
        case "explicit":
            return "text-red-500"
            break
        default:
            return "hidden"
            break
    }
}
%>